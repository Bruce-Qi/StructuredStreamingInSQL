CREATE TABLE SocketTable(
    word string,
    wordcount int
)WITH(
    type='socket',
    host='hadoop-sh1-core1',
    process='2s',
    port='9998'
);

create SINK console(
)WITH(
    type='console',
    outputmode='complete',
    process='2s'
);

insert into console select word,count(wordcount) from SocketTable group by word;